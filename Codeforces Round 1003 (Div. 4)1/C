#include<iostream>
#include<cstring>
using namespace std;
const int N = 1e6;
int a[N];
void gengxing(int* a, int b) {
	int temp = b - *a;
	if (temp < *a && temp >= *(a - 1)) *a = temp;
	
}
bool check(int* a, int n) {
	
	for (int i = 1; i <= n-1; i++) {
		if (a[i] > a[i + 1]) return false;
	}
	return true;

	
}
int main() {
	
	int t; cin >> t; 
	while (t--) {
		int n, m; cin >> n >> m;
		int b;
		for (int i = 1; i <= n; i++) {
			cin >> a[i];
		}
		cin >> b;
		a[1] = min(a[1], b - a[1]);
		if (n == 1)cout << "YES"<<'\n';
		else {
			for (int i = 2; i <= n; i++) {
				int x = min(a[i], b - a[i]);
				if (x >= a[i - 1]) a[i] = x;
			}
			
			if (check(a, n)) cout << "YES" << '\n';
			else cout << "NO" << '\n';
		}

		
	}
	
	return 0;
}
